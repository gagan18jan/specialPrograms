#include<stdio.h>
#include<conio.h>
#define max 5
int que[max],front=-1,rear=-1;
voidenqueue(intval)
{
	if(front==0 && rear==max-1)
	{
		printf("Overflow");
		return;
	}	
	else if(front==-1 && rear==-1)
		front=rear=0;
	else if(rear==max-1 && front!=0)
		rear=0;
	else
		rear++;
	que[rear]=val;
}
voiddequeue()
{
	if(front==-1)
	{
		printf("Underflow");
		return;
	}
	printf("Deleted value is %d",que[front]);
	que[front]=0;
	if(front==rear)
		front=rear=-1;
	else if(front==-1)
		front=0;
	else
		front++;
}
void display()
{
	inti;
	if(front==-1)
		printf("Circular Queue is empty");
	else
		for(i=0;i<=max-1;i++)
			printf("%d\t",que[i]);
}
void main()
{
	clrscr();
	intval,option;
	do
	{
		printf("\n1.Insert\n2.Delete\n3.Display\n4.Exit");
		printf("\nEnter your option:");
		scanf("%d",&option);
		switch(option)
		{
		case 1:printf("Enter the value to be added:");
			scanf("%d",&val);
			enqueue(val);
			break;
		case 2:dequeue();
		break;
		case 3:display();
		break;
		case 4:break;
		default:printf("Invalid");
		}
	}while(option!=4);
	getch();
}
